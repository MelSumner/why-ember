{
  "name": "why-ember",
  "version": "0.0.0",
  "private": true,
  "description": "The whyember.com website",
  "repository": "https://github.com/MelSumner/why-ember.git",
  "license": "MIT",
  "author": "Melanie Sumner <melanie.sumner@hey.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.6.0",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-a11y-refocus": "2.3.0",
    "ember-auto-import": "2.2.4",
    "ember-cli": "3.28.6",
    "ember-cli-autoprefixer": "2.0.0",
    "ember-cli-babel": "7.26.11",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-page-title": "7.0.0",
    "ember-prism": "0.13.0",
    "ember-qunit": "8.0.2",
    "ember-resolver": "8.1.0",
    "ember-source": "3.28.9",
    "ember-template-lint": "3.16.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-ember": "10.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-qunit": "6.2.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "qunit": "2.17.2",
    "qunit-dom": "3.2.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "ember-cli-netlify": "0.4.1",
    "ember-test-helpers": "0.6.3",
    "highlightjs": "9.16.2",
    "webpack": "5.65.0"
  }
}
